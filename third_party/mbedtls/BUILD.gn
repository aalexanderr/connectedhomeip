declare_args() {
  # Use a different target, such as a prebuilt MbedTLS.
  mbedtls_target = ""
}

if (mbedtls_target != "") {
  group("mbedtls") {
    public_deps = [ mbedtls_target ]
  }
} else {
  import("mbedtls.gni")

  mbedtls_target("mbedtls") {
  }
}
