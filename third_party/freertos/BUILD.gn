declare_args() {
  # Use a different target, such as a prebuilt FreeRTOS
  freertos_target = ""
}

if (freertos_target != "") {
  group("freertos") {
    public_deps = [ freertos_target ]
  }
} else {
  import("freertos.gni")

  freertos_target("freertos") {
  }
}
