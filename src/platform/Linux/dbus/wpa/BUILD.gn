import("//build_overrides/chip.gni")

import("${chip_root}/build/chip/linux/gdbus_library.gni")

gdbus_library("wpa") {
  sources = [
    "DBusWpa.xml",
    "DBusWpaBss.xml",
    "DBusWpaInterface.xml",
    "DBusWpaNetwork.xml",
  ]

  c_namespace = "Wpa"
  c_generate_object_manager = false
  dbus_out_dir = "platform/Linux/dbus/wpa"
}
