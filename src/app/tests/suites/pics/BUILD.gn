import("//build_overrides/build.gni")
import("//build_overrides/chip.gni")

static_library("pics") {
  output_name = "libPICS"

  sources = [
    "PICSBooleanExpressionParser.cpp",
    "PICSBooleanExpressionParser.h",
    "PICSBooleanReader.cpp",
    "PICSBooleanReader.h",
    "PICSNormalizer.cpp",
    "PICSNormalizer.h",
