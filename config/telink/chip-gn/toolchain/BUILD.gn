import("//build/toolchain/gcc_toolchain.gni")
import("//build_overrides/chip.gni")

declare_args() {
  zephyr_ar = ""
  zephyr_cc = ""
  zephyr_cxx = ""
}

gcc_toolchain("zephyr") {
  ar = zephyr_ar
  cc = zephyr_cc
  cxx = zephyr_cxx

  toolchain_args = {
    current_os = "zephyr"
    is_clang = false
  }
}
