import("//build/toolchain/arm_gcc/arm_toolchain.gni")

declare_args() {
  mbed_ar = ""
  mbed_cc = ""
  mbed_cxx = ""
}

gcc_toolchain("mbed") {
  ar = mbed_ar
  cc = mbed_cc
  cxx = mbed_cxx

  toolchain_args = {
    current_os = "mbed"
    is_clang = false
  }
}
