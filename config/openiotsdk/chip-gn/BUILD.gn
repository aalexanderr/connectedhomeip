import("//build_overrides/chip.gni")
import("${chip_root}/build/chip/tests.gni")

group("openiotsdk") {
  deps = [ "${chip_root}/src/lib" ]

  if (chip_build_tests) {
    deps += [ "${chip_root}/src:tests" ]
  }
}

group("default") {
  deps = [ ":openiotsdk" ]
}
