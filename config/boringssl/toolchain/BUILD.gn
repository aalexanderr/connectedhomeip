import("//build_overrides/build.gni")

import("${build_root}/toolchain/gcc_toolchain.gni")

gcc_toolchain("${host_os}_${host_cpu}_clang_boringssl") {
  toolchain_args = {
    current_os = host_os
    current_cpu = host_cpu
    is_clang = true
    chip_crypto = "boringssl"
  }
}
